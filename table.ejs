<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <title>Tabela</title>
    <meta charset="utf-8">
  </head>
  <body>
    <table>
        <thead>
            <% header.forEach(col => {%>
                <th>  <%= col %>  </th>
            <% }) %>
        </thead>
        <tbody>
            <% rows.forEach(row => {%>
                <tr>  
                    <% row.forEach(element => { %>
                   <td>  <%= element %>   </td> 
                <% }) %>  </tr>
            <% }) %>

        </tbody>
    </table>
  </body>
</html>